<launch>
<node pkg="tf" type="static_transform_publisher" name="tf_world" args="0 0 0 0 0 0 map world 100" />
<node pkg="tf" type="static_transform_publisher" name="tf_liadr" args="0 0 0.5 0 0 3.141592654 world livox_frame 100" />
  <!-- Elevation mapping node -->
  <node pkg="elevation_mapping_cupy" type="elevation_mapping_node" name="elevation_mapping" output="screen">
    <rosparam command="load" file="$(find elevation_mapping_cupy)/config/parameters_ceshi_livox.yaml" />
  </node>
</launch>
